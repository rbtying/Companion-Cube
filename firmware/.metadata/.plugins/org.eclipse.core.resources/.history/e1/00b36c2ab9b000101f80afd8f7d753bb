#ifndef CONTROL_STRUCT_H_
#define CONTROL_STRUCT_H_

struct control_data {
	double lSet;
	double rSet;
	double lMeas;
	double rMeas;
	long lEnc;
	long rEnc;
	PowerMonitor batt;
	double yawRate;
	double yawVal;
	Servo pan;
	Servo tilt;
	double lPID[3];
	double rPID[3];
};
#endif /* CONTROL_STRUCT_H_ */
